          },
          "publishTime": "2017-05-26T07:17:30.693Z",
          "flagContentUri": "https://www.google.com/local/content/rap/report?postId=ChdDSUhNMG9nS0VJQ0FnSURReV9xdTNnRRAB&d=17924085&t=1",
          "googleMapsUri": "https://www.google.com/maps/reviews/data=!4m6!14m5!1m4!2m3!1sChdDSUhNMG9nS0VJQ0FnSURReV9xdTNnRRAB!2m1!1s0x4887c7b1d1315c63:0x25318d9d4e3ddc3d"
